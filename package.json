{
  "name": "focus-lock",
  "version": "0.1.0",
  "description": "Focus-Lock Gamified Productivity App for Windows",
  "main": "dist/main/index.js",
  "homepage": "./",
  "author": "Dev Team",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -w --project tsconfig.main.json",
    "dev:renderer": "react-scripts start",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc --project tsconfig.main.json",
    "build:renderer": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/aria-query": "^5.0.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.31",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
